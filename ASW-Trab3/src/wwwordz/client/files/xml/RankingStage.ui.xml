<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.table-style {	
		width: 80%;
		height: 80%;
		overflow: scroll;
		padding-bottom: 5% 5% 5% 5%;
		border: 2xp solid gray;
		font-size: x-large;
		color: black;
	}
	.table-page-style{
		font-size: large;
	}
	.table-style tr{
		border-top: 2px solid black;
		border-bottom: 2px solid black;
		padding: 2% 0 2% 0;
	}
	.table-style td{
		border-left: 2px solid gray;
		border-right: 2px solid gray;
		border-top: 1xp solib black;
		padding: 0 2% 0 2%;
		text-align: centre;
		vertical-align: centre;
	}
	</ui:style>
	<g:HTMLPanel styleName="{style.table-page-style}">
		<div align="centre">
			<g:FlexTable ui:field="table" styleName="{style.table-style}"></g:FlexTable>
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 